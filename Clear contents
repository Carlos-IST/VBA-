'   Descripción: Botón para borrar contenido de las celdas (mantiene formatos)

Sub BorraData()
    Range("I5:I111").ClearContents
    Range("K35:K111").ClearContents
    
End Sub

Sub ClearOrden()
    Range("B4:M110").ClearContents
End Sub

Sub BorraAux()
    Range("K1:K5476").ClearContents
    Range("Q1:Q5476").ClearContents
End Sub

Sub BorraUbicaciones()
    Range("C123:Q200").ClearContents
End Sub

